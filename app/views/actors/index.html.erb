<h1>Listing Actors</h1>

 <%= link_to 'Want to provide information about an actor?', new_actor_path %>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
 
  <% @actors.each do |actor| %>
    <tr>
      <td><%= actor.first_name %></td>
      <td><%= actor.last_name %></td>
    </tr>
  <% end %>
</table>